{
  "name": "image-cropper-next",
  "private": false,
  "version": "0.0.2",
  "description": "A Vue 3 image cropper component for Vite",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "style": "dist/style.css",
  "type": "module",
  "typings": "dist/types/index.d.ts",
  "keywords": [
    "image-cropper-next",
    "vue3",
    "typescript",
    "vite",
    "vue",
    "cropper",
    "marvin"
  ],
  "scripts": {
    "dev": "vite",
    "clean": "rimraf ./dist",
    "build": "pnpm clean && pnpm dts && vite build && pnpm generate:types",
    "dts": "rimraf ./tsc/type && vue-tsc --declaration --emitDeclarationOnly -p ./tsconfig.declaration.json",
    "generate:types": "rimraf ./dist/types && node scripts/generate-types.cjs",
    "preview": "vite preview"
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "author": {
    "name": "Marvin",
    "email": "454846659@qq.com"
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@types/node": "^20.8.7",
    "@vitejs/plugin-vue": "^4.2.3",
    "rimraf": "^5.0.5",
    "sass": "^1.69.4",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vue-tsc": "^1.8.5"
  }
}
